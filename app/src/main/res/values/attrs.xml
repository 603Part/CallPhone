<resources>
    <declare-styleable name="MyCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

    <declare-styleable name="NodeProgress">
        <attr name="barSpeed">
            <enum name="normal" value="40"/>
            <enum name="slow" value="80"/>
            <enum name="fast" value="10"/>
        </attr>
        <attr name="barDotsCount" format="integer"/>
        <attr name="barDotsRadius" format="dimension|reference"/>
        <attr name="barProgressWidth" format="dimension|reference"/>
        <attr name="barBackColor" format="color|reference"/>
        <attr name="barFrontColor" format="color|reference"/>
    </declare-styleable>
</resources>
